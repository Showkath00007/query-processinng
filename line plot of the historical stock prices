!pip install yfinance

import pandas as pd
import yfinance as yf
import matplotlib.pyplot as plt
ticker_symbol = 'GOOGL'
start_date = '2023-01-01'
end_date = '2024-01-01'
df_stock = yf.download(ticker_symbol, start=start_date, end=end_date)
print(f"Historical stock data for {ticker_symbol} from {start_date} to {end_date}:")
print(df_stock.head())
plt.figure(figsize=(12, 6))
plt.plot(df_stock.index, df_stock['Close'], label='GOOGL Close Price', color='blue')
plt.title(f'Alphabet Inc. ({ticker_symbol}) Stock Price History ({start_date} to {end_date})')
plt.xlabel('Date')
plt.ylabel('Close Price (USD)')
plt.grid(True)
plt.legend()
plt.tight_layout()
plt.show()








